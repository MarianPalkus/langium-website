{
  "name": "langium-website-core",
  "version": "2.0.0",
  "type": "module",
  "description": "Bundling complex sources for hugo",
  "author": "TypeFox",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./bundle": {
      "types": "./dist/index.d.ts",
      "default": "./bundle/monaco-editor-wrapper-bundle/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/index"
      ],
      "bundle": [
        "dist/index"
      ]
    }
  },
  "files": [
    "dist",
    "bundle",
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "clean": "shx rm -rf ./bundle ./dist",
    "compile": "tsc",
    "build:bundle": "vite --config vite.bundle.ts build",
    "build": "npm run clean && npm run compile && npm run build:bundle"
  },
  "devDependencies": {
    "@types/react": "~18.2.79",
    "@types/react-dom": "~18.2.25",
    "@types/vscode": "~1.88.0",
    "typescript": "~5.4.5",
    "vite": "~5.2.10"
  },
  "dependencies": {
    "@codingame/monaco-vscode-keybindings-service-override": "~4.3.2",
    "@typefox/monaco-editor-react": "~3.2.1",
    "monaco-editor": "npm:@codingame/monaco-vscode-editor-api@~4.3.2",
    "monaco-editor-wrapper": "~4.2.1",
    "monaco-languageclient": "~8.3.1",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "vscode": "npm:@codingame/monaco-vscode-api@~4.3.2",
    "vscode-languageserver": "~9.0.1"
  },
  "volta": {
    "node": "18.19.1",
    "npm": "10.2.4"
  }
}
